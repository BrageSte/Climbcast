{
  "name": "Climbcast",
  "description": "A beautiful Norwegian climbing weather application that helps climbers find the best crags based on current and forecasted weather conditions",
  "version": "1.0.0",
  "author": "Brage Steen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrageSte/ClimbCast.git"
  },
  "keywords": [
    "climbing",
    "weather",
    "norway",
    "crags",
    "forecast",
    "bouldering",
    "sport-climbing",
    "outdoor",
    "met-norway",
    "supabase"
  ],
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "import-osm": "tsx src/scripts/importOSMCrags.ts",
    "calculate-aspects": "tsx src/scripts/calculateMissingAspects.ts",
    "recalculate-aspects": "tsx src/scripts/recalculateAllAspects.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.90.6",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/leaflet": "^1.9.21",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "dotenv": "^17.2.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
